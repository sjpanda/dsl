<?xml version="1.0" encoding="ASCII"?>
<webapp:WebApp
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:webapp="http://webapp/1.0"
    xsi:schemaLocation="http://webapp/1.0 ../model/WebApp.ecore"
    name="site"
    framework="struts">
  <appConfig/>
  <webConfig/>
  <view>
    <page name="MyPage"/>
  </view>
  <model databaseName="jee"
      url="localhost:3306"
      userName="toto">
    <table name="contact"
        charset="UTF8">
      <column
          name="id"
          useZeroFill="true"
          type="INTEGER"
          defaultValue=""/>
      <column
          name="money"
          type="FLOAT">
        <detail
            precision="7"
            scale="2"/>
      </column>
      <column
          name="year"
          size="4"
          type="YEAR"/>
    </table>
  </model>
  <controller>
    <validator
        name="anActionValidator"/>
    <action
        validator="//@controller/@validator.0"
        name="anAction"/>
  </controller>
</webapp:WebApp>
