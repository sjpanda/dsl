<?xml version="1.0" encoding="ASCII"?>
<webapp:WebApp
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:webapp="http://webapp/1.0"
    xsi:schemaLocation="http://webapp/1.0 ../model/WebApp.ecore"
    name="My first web app"
    framework="Struts">
  <appConfig/>
  <webConfig/>
  <library/>
  <view>
    <page name="creationContact"/>
    <page name="success"/>
    <page name="error"/>
  </view>
  <model databaseName="jee"
      url="localhost:3306"
      userName="toto">
    <table name="contact"
        charset="UTF8">
      <column
          name="id"
          isNotNull="true"
          size="10"
          useZeroFill="true"
          type="INTEGER"/>
      <column
          name="firstname"
          size="255"
          type="VARCHAR"/>
      <column
          name="lastname"
          isNotNull="true"
          size="255"
          type="VARCHAR"/>
      <column
          name="age"
          type="INTEGER"/>
      <column
          name="email"
          size="255"
          type="VARCHAR"/>
      <column
          name="groupid"/>
      <constraint>
        <primaryKey
            column="//@model/@table.0/@column.0"/>
        <unique
            column="//@model/@table.0/@column.4 //@model/@table.0/@column.2"/>
        <foreignKey
            externalColumn="//@model/@table.1/@column.0"
            externalTable="//@model/@table.1"
            internalColumn="//@model/@table.0/@column.5">
          <onDelete/>
          <onUpdate
              behavior="RESTRICT"/>
        </foreignKey>
      </constraint>
    </table>
    <table name="team"
        charset="UTF8">
      <column
          name="gid"/>
      <column
          name="name"
          size="255"
          type="VARCHAR"/>
      <column
          name="budget"
          type="DECIMAL">
        <detail
            precision="7"
            scale="2"/>
      </column>
      <constraint>
        <primaryKey
            column="//@model/@table.1/@column.0"/>
        <check
            expr="gid > 5"/>
      </constraint>
    </table>
  </model>
  <controller>
    <validator
        name="AddContactValidationForm"
        package="p1"/>
    <action
        name="AddContactAction"/>
  </controller>
  <resource>
    <image/>
    <file/>
    <propertie
        name="messages"/>
  </resource>
</webapp:WebApp>
